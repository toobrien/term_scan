{
    "db_path": "../v2/srf.db",
    "default_scan_batch": [
        {
            "name": "2-leg calendar",
            "contract": "LN",
            "params": {
                "type": "calendar",
                "data_range": [ "2010-01-01", "2025-01-01" ],
                "result_limit": null,
                "legs": [ 
                    [ "F", "Z", "0", "1", "A" ],
                    [ "+1", "Z", "+0", "1", "B" ]
                ],
                "filters": [
                    { 
                        "type": "year", 
                        "range": [ 2021, 2022 ] 
                    },
                    { 
                        "type": "price", 
                        "mode": "percentile", 
                        "range": [ [ 0.95, 1.0 ], [ 0.0, 0.05 ] ]
                    }
                ]
            }
        },
        {
            "name": "1-wide butterfly",
            "contract": "ED",
            "params": {
                "type": "sequence",
                "data_range": [ "2010-01-01", "2035-01-01" ],
                "result_limit": 50,
                "legs": [
                    [ "0", "10", "A" ],
                    [ "+1", "+1", "B" ],
                    [ "+0", "+0", "B" ],
                    [ "+1", "+1", "A" ]
                ]
            },
            "filters": [
                {
                    "type": "year", 
                    "range": [ 2021, 2031 ]
                },
                { 
                    "type": "price",
                    "mode": "percentile",
                    "range": [ [ 0.9985, 1.0 ], [ 0.0, 0.0015 ] ]
                }
            ]
        }
    ]
}